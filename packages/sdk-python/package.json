{
  "name": "@agentmark/sdk-python",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "python3 -m venv .venv && .venv/bin/pip install -q -e '.[dev]'",
    "test": ".venv/bin/pytest",
    "test:watch": ".venv/bin/pytest --watch",
    "lint": "test -f .venv/bin/ruff && .venv/bin/ruff check src tests || echo 'Skipping lint: venv not set up'",
    "lint:fix": ".venv/bin/ruff check --fix src tests",
    "typecheck": ".venv/bin/mypy src --strict",
    "format": ".venv/bin/ruff format src tests",
    "clean": "rm -rf .venv dist *.egg-info .pytest_cache .mypy_cache .ruff_cache"
  }
}
