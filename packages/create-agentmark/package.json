{
  "name": "create-agentmark",
  "version": "0.4.0",
  "type": "module",
  "description": "Create AgentMark projects with npm create agentmark",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./templates": "./dist/utils/examples/templates/index.js"
  },
  "scripts": {
    "build": "node -e \"require('fs').rmSync('./dist', { recursive: true, force: true })\" && tsup && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
    "test": "vitest run",
    "lint": "eslint .",
    "prepack": "node -e \"try { require('fs').chmodSync('dist/index.js', '755') } catch {}\"",
    "prepare": "node -e \"try { require('fs').chmodSync('dist/index.js', '755') } catch {}\""
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "agentmark",
    "ai",
    "agents",
    "create-agentmark",
    "scaffolding"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@agentmark/shared-utils": "*",
    "fs-extra": "^11.2.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.5.1",
    "typescript": "^5",
    "vitest": "^3.1.4"
  },
  "directories": {
    "test": "test"
  },
  "types": "./dist/index.d.ts"
}
