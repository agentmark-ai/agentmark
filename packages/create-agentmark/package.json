  "description": "Create AgentMark projects with npm create agentmark",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "build": "node -e \"require('fs').rmSync('./dist', { recursive: true, force: true })\" && tsc && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
