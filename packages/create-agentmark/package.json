{
  "name": "create-agentmark",
  "version": "0.8.2",
  "type": "module",
  "description": "Create AgentMark projects with npm create agentmark",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./templates": "./dist/utils/examples/templates/index.js"
  },
  "scripts": {
    "build": "node -e \"require('fs').rmSync('./dist', { recursive: true, force: true })\" && tsup --config tsup.main.config.ts && tsup --config tsup.templates.config.ts",
    "test": "vitest run",
    "test:unit": "vitest run test/unit/",
    "test:integration": "vitest run test/integration/",
    "test:watch": "vitest",
    "lint": "eslint ."
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "agentmark",
    "ai",
    "agents",
    "create-agentmark",
    "scaffolding"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@agentmark-ai/shared-utils": "0.3.0",
    "fs-extra": "^11.2.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.5.1",
    "typescript": "^5",
    "vitest": "^3.1.4"
  },
  "directories": {
    "test": "test"
  },
  "types": "./dist/index.d.ts"
}
