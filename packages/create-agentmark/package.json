    "prepare": "node -e \"try { require('fs').chmodSync('dist/index.js', '755') } catch {}\""
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "agentmark",
    "ai",
    "agents",
    "create-agentmark",
    "scaffolding"
  ],
