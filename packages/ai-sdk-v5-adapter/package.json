{
  "name": "@agentmark-ai/ai-sdk-v5-adapter",
  "version": "1.2.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./runner": {
      "import": {
        "types": "./dist/runner.d.ts",
        "default": "./dist/runner.js"
      },
      "require": {
        "types": "./dist/runner.d.cts",
        "default": "./dist/runner.cjs"
      }
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup src/index.ts src/runner.ts --format cjs,esm --dts --clean",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@agentmark-ai/loader-file": "*",
    "@agentmark-ai/prompt-core": "0.2.0",
    "@agentmark-ai/sdk": "1.0.6",
    "@agentmark-ai/templatedx": "*",
    "@ai-sdk/mcp": "^0.0.8",
    "@ai-sdk/openai": "^2.0.0",
    "@types/mdast": "^4.0.0",
    "@types/node": "^22.8.1",
    "@types/react": "^18",
    "ai": "^5.0.52",
    "cpy-cli": "^5.0.0",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2",
    "vitest": "^3.0.8"
  },
  "peerDependencies": {
    "@agentmark-ai/prompt-core": "*",
    "@agentmark-ai/sdk": "*",
    "@ai-sdk/mcp": "^0.0.8",
    "ai": "^5.0.52"
  },
  "files": [
    "dist/"
  ]
}
