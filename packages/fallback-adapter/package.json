    "lint": "eslint ."
  },
  "devDependencies": {
    "@agentmark-ai/loader-file": "workspace:*",
    "@agentmark-ai/prompt-core": "workspace:*",
    "@types/node": "^22.8.1",
    "@types/react": "^18",
    "cpy-cli": "^5.0.0",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2",
    "vitest": "^3.0.8"
  },
  "peerDependencies": {
    "@agentmark-ai/prompt-core": "*"
  },
  "files": [
    "dist/"
  ]
