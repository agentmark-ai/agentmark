{
  "name": "@agentmark-ai/prompt-core",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./test-utils": {
      "import": {
        "types": "./dist/test-utils.d.ts",
        "default": "./dist/test-utils.js"
      },
      "require": {
        "types": "./dist/test-utils.d.cts",
        "default": "./dist/test-utils.cjs"
      }
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup && npx cpy ../../README.md ../../assets ./",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@agentmark-ai/loader-file": "*",
    "@types/node": "^22.8.1",
    "@types/react": "^18",
    "cpy-cli": "^5.0.0",
    "dotenv": "^16.5.0",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "@agentmark-ai/templatedx": "workspace:*",
    "zod": "^3.23.8"
  },
  "files": [
    "dist/",
    "assets/",
    "README.md"
  ]
}
