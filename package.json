{
  "name": "agentmark",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "web:dev": "yarn workspace @agentmark/web dev",
    "web:build": "yarn workspace @agentmark/web build",
    "db:generate": "yarn workspace @agentmark/web prisma generate",
    "db:push": "yarn workspace @agentmark/web prisma db push",
    "db:migrate": "yarn workspace @agentmark/web prisma migrate dev"
  },
  "devDependencies": {
    "turbo": "^1.10.12"
  },
  "packageManager": "yarn@3.5.4"
}
